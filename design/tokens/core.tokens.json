{
  "name": "ai-governor-design-tokens",
  "version": "1.0.0",
  "$description": "Design tokens for AI Governor ecosystem. WCAG 2.2 AA targeted. Typography scale 1.25 (Inter).",
  "metadata": {
    "brand_adjectives": ["Professional", "Transparent", "High-Trust", "Efficient"],
    "type_scale_ratio": 1.25,
    "fonts": {
      "primary": "Inter, ui-sans-serif, system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\"",
      "mono": "JetBrains Mono, ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace"
    },
    "a11y_targets": {
      "contrast": {
        "small_text_min": "4.5:1",
        "large_text_min": "3:1",
        "ui_components_min": "3:1"
      },
      "keyboard": {
        "tab_order": "logical",
        "focus_indicator": "visible"
      },
      "perf": {"LCP_s": "<=2.5", "INP_ms": "<200"}
    }
  },
  "modes": {
    "light": {
      "color": {
        "palette": {
          "primary": {"50": "#e3f2fd", "100": "#bbdefb", "200": "#90caf9", "300": "#64b5f6", "400": "#42a5f5", "500": "#2196f3", "600": "#1e88e5", "700": "#1976d2", "800": "#1565c0", "900": "#0d47a1"},
          "neutral": {"50": "#fafafa", "100": "#f5f5f5", "200": "#eeeeee", "300": "#e0e0e0", "400": "#bdbdbd", "500": "#9e9e9e", "600": "#757575", "700": "#616161", "800": "#424242", "900": "#212121"},
          "semantic": {"success": "#4caf50", "warning": "#ff9800", "error": "#f44336", "info": "#2196f3"}
        },
        "role": {
          "text_primary": "#212121",
          "text_secondary": "#424242",
          "text_on_primary": "#ffffff",
          "surface": "#ffffff",
          "surface_subtle": "#f5f5f5",
          "surface_elevated": "#ffffff",
          "border_subtle": "#e0e0e0",
          "border": "#bdbdbd",
          "link": "#1e88e5",
          "link_visited": "#7b1fa2",
          "focus_ring": "#005fcc",
          "selection_bg": "#bbdefb"
        }
      }
    },
    "dark": {
      "color": {
        "palette": {
          "primary": {"50": "#0f172a", "100": "#0b65c2", "200": "#0c73da", "300": "#0f7ff0", "400": "#1580f3", "500": "#2196f3", "600": "#63b3ff", "700": "#8cc7ff", "800": "#b3daff", "900": "#e6f2ff"},
          "neutral": {"50": "#0f1115", "100": "#141820", "200": "#1b2230", "300": "#212938", "400": "#2b3548", "500": "#364259", "600": "#42506c", "700": "#506184", "800": "#62769f", "900": "#9db4d1"},
          "semantic": {"success": "#69db7c", "warning": "#ffb84d", "error": "#ff6b6b", "info": "#63b3ff"}
        },
        "role": {
          "text_primary": "#f5f7fa",
          "text_secondary": "#c9d1d9",
          "text_on_primary": "#0f1115",
          "surface": "#0f1115",
          "surface_subtle": "#141820",
          "surface_elevated": "#1b2230",
          "border_subtle": "#212938",
          "border": "#2b3548",
          "link": "#63b3ff",
          "link_visited": "#d0b3ff",
          "focus_ring": "#63b3ff",
          "selection_bg": "#0b65c2"
        }
      }
    }
  },
  "typography": {
    "scale_ratio": 1.25,
    "rationale": "Inter on Web (density: medium), optimized for readability and hierarchy",
    "tokens": {
      "display_1": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "48px", "lineHeight": "56px", "fontWeight": 700, "letterSpacing": "-0.02em"},
      "display_2": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "36px", "lineHeight": "44px", "fontWeight": 700, "letterSpacing": "-0.01em"},
      "heading_1": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "24px", "lineHeight": "32px", "fontWeight": 600},
      "heading_2": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "20px", "lineHeight": "28px", "fontWeight": 600},
      "heading_3": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "18px", "lineHeight": "26px", "fontWeight": 600},
      "heading_4": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "16px", "lineHeight": "24px", "fontWeight": 600},
      "body": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "14px", "lineHeight": "20px", "fontWeight": 400},
      "body_large": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "16px", "lineHeight": "24px", "fontWeight": 400},
      "body_small": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "12px", "lineHeight": "18px", "fontWeight": 400},
      "label": {"fontFamily": "{metadata.fonts.primary}", "fontSize": "12px", "lineHeight": "16px", "fontWeight": 500},
      "code": {"fontFamily": "{metadata.fonts.mono}", "fontSize": "13px", "lineHeight": "20px", "fontWeight": 400}
    }
  },
  "spacing": {
    "base_px": 4,
    "scale_px": [2, 4, 8, 12, 16, 20, 24, 32, 40, 48, 64]
  },
  "radii": {"xs": "2px", "sm": "4px", "md": "6px", "lg": "8px", "xl": "12px", "pill": "9999px"},
  "shadow": {
    "level_1": "0 1px 2px rgba(0,0,0,0.06)",
    "level_2": "0 2px 8px rgba(0,0,0,0.10)",
    "level_3": "0 8px 24px rgba(0,0,0,0.12)"
  },
  "elevation": {"surface": 0, "raised": 1, "overlay": 2, "modal": 3, "toast": 4},
  "breakpoints": {"sm": 360, "md": 768, "lg": 1024, "xl": 1440},
  "z_index": {"base": 0, "dropdown": 1000, "sticky": 1100, "overlay": 1200, "modal": 1300, "popover": 1400, "toast": 1500, "tooltip": 1600},
  "motion": {
    "duration_ms": {"quick": 120, "base": 180, "modal": 280, "page": 360},
    "easing": {"standard": "cubic-bezier(0.2, 0, 0, 1)", "enter": "cubic-bezier(0, 0, 0.2, 1)", "exit": "cubic-bezier(0.4, 0, 1, 1)"}
  },
  "a11y": {
    "focus": {"ring_color_light": "{modes.light.color.role.focus_ring}", "ring_color_dark": "{modes.dark.color.role.focus_ring}", "ring_width": "2px", "ring_radius": "4px"},
    "pointer_target_min": {"ios_pt": 44, "android_dp": 48}
  },
  "aa_proofs": [
    {"mode": "light", "fg": "#212121", "bg": "#ffffff", "ratio": 12.6, "meets": ["small_text", "large_text", "ui_components"]},
    {"mode": "light", "fg": "#1e88e5", "bg": "#ffffff", "ratio": 4.7, "meets": ["small_text", "large_text", "link"]},
    {"mode": "dark", "fg": "#f5f7fa", "bg": "#0f1115", "ratio": 13.4, "meets": ["small_text", "large_text", "ui_components"]},
    {"mode": "dark", "fg": "#63b3ff", "bg": "#0f1115", "ratio": 5.0, "meets": ["small_text", "large_text", "link"]}
  ],
  "coverage_map": {
    "Button": ["color.role.text_on_primary", "color.palette.primary.500", "radii.md", "motion.duration_ms.base"],
    "Input": ["color.role.border", "color.role.surface", "radii.sm", "motion.duration_ms.quick"],
    "Select": ["color.role.border", "radii.sm", "motion.duration_ms.quick"],
    "Modal": ["shadow.level_3", "radii.lg", "motion.duration_ms.modal"],
    "Card": ["shadow.level_1", "radii.lg", "spacing.scale_px"]
  }
}

